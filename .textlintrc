{
  "filters": {
    "node-types": {
      "nodeTypes": [
        "Code",
        "Paragraph"
      ]
    }
  },
  "rules": {
    "preset-ja-technical-writing": {
      "no-exclamation-question-mark": false,
      "sentence-length": {
        "max": 200
      }
    },
    "spellcheck-tech-word": true,
    "textlint-rule-ginger": {
      "skipPatterns": [
        "`(.*)`"
      ]
    },
    "textlint-rule-spellchecker": {
      "skipWords": [
        "takeokunn",
        "mtime",
        "ctime",
        "nohideblocks",
        "fluent",
        "fluentd",
        "laravel",
        "sbcl",
        "netstat",
        "npm",
        "axios",
        "Xlib.error.DisplayConnectionError",
        "webpacker",
        "circleci",
        "capisorano",
        "Redash",
        "jq",
        "Golang",
        "dependabot",
        "phpstan",
        "eslint",
        "rubocop",
        "session",
        "ghq",
        "Dotfiles",
        "peco",
        "bitwarden",
        "UID",
        "AZIK",
        "wanderlust",
        "emacs",
        "elisp",
        "dotfiles",
        "exwm",
        "magit",
        "Homebrew",
        "s",
        "DynASM",
        "pecl",
        "Webpacker",
        "Fluentd",
        "aws",
        "ecs",
        "Neovim",
        "phperkaigi",
        "phper",
        "OPENLOGI",
        "emacser",
        "Anker",
        "Datadog",
        "ob-phpstan",
        "org-babel",
        "babel",
        "melpa",
        "gpg",
        "gnupg"
      ],
      "skipRegExps": [
        "([0-9A-F]{8})-([0-9A-F]{4})-([0-9A-F]{4})-([0-9A-F]{4})-([0-9A-F]{12})"
      ]
    },
    "textlint-rule-write-good": true,
    "preset-ja-spacing": true,
    "textlint-rule-no-dropping-the-ra": true,
    "ginger": false,
    "prh": {
      "rulePaths": [
        "node_modules/prh/prh-rules/media/WEB+DB_PRESS.yml",
        "node_modules/prh/prh-rules/media/techbooster.yml"
      ]
    }
  },
  "plugins": [
    "org"
  ]
}
